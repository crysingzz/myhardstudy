#include <bits/stdc++.h>

using namespace std;

int main() {
	string str;
	cin >> str;
	if (str[0] == 'a' or str[0] == 'h' or str[1] == '1' or str[1] == '8')
	{
		if (str[0] == 'a' and (str[1] == '1' or str[1] == '8') or str[0] == 'h' and (str[1] == '1' or str[1] == '8'))
		{
			cout << 3;
		}
		else
		{
			cout << 5;
		}
		
	}
	else
	{
		cout << 8;
	}
/*we need to know how many moves the king has
we can detect, that king has 8 moves in every point, except sides
we have 3 case:
	1. king in the corner -> 3 moves
	2. king in the side, but not in the corner -> 5 moves
	3. king on board -> 8 moves
*/
}
